{
  "hosting": {
    "public": "dist/browser",
    "rewrites": [
      {
        "source": "**",
        "destination": "https://fctapp.yudi.me/",
        "type": 301
      }
    ]
  },
  "firestore": {
    "rules": "firestore.rules"
  },
  "storage": {
    "rules": "storage.rules"
  },
  "emulators": {
    "firestore": {
      "port": "8081"
    },
    "storage": {
      "port": "9199"
    },
    "hosting": {
      "port": "5002"
    },
    "auth": {
      "port": "9099"
    },
    "ui": {
      "enabled": true
    },
    "functions": {
      "port": 5001
    }
  },
  "functions": {
    "predeploy": "npm --prefix \"$RESOURCE_DIR\" run build",
    "source": "functions"
  }
}